  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def test(X_test, y_test):\n",
    "    model_df = pd.DataFrame(columns=['LinearSVC'],\n",
    "                                     index=['True Positive','True Negative', 'False Positive', \n",
    "                                            'False Negative', 'Recall', 'Precision', \n",
    "                                            'Accuracy', 'F1 score'])\n",
    "    \n",
    "    model_pickle = pk.load(open('best_model.sav', 'rb')) # Model Linear SVC\n",
    "    vectorizer_pickle = pk.load(open('vectorizer.sav', 'rb')) # Load the TF-IDF Vectorizer\n",
    "\n",
    "    X_vec = vectorizer_pickle.transform(X_test) # use vectorizer to test sample\n",
    "    pred_test = model_pickle.predict(X_vec) # predict test labels\n",
    "                                     \n",
    "    tn, fp, fn, tp = confusion_matrix(y_test, pred_test).ravel()\n",
    "    accuracy = metrics.accuracy_score(y_test, pred_test) * 100\n",
    "    recall = (tp / (tp + fn)) * 100\n",
    "    precision = (tp / (tp + fp)) * 100\n",
    "    f1 = (2*recall*precision)/(recall+precision)\n",
    "    model_df.iloc[:,0] = [tp, tn, fp, fn, recall, precision, accuracy, f1]\n",
    "    return model_df "
   ]
  },
